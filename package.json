{
  "name": "tourismguide",
  "version": "1.0.0",
  "description": "Local Tourism Guide Webapp for WEB Technologies course at ORT Braude",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "concurrently \"npm run start:server\" \"npm run start:ui\"",
    "start:ui": "cd Tourism-Guide-UI && npm run start",
    "start:server": "cd Tourism-Guide-SERVER && npm run start",
    "build": "npm run build:ui && npm run build:server",
    "build:ui": "cd Tourism-Guide-UI && npm run build",
    "build:server": "cd Tourism-Guide-SERVER && npm run build",
    "install:all": "npm install && npm run install:ui && npm run install:server",
    "install:ui": "cd Tourism-Guide-UI && npm install",
    "install:server": "cd Tourism-Guide-SERVER && npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eladgl/tourismGuide.git"
  },
  "author": "Elad Goldenberg, Sapir Itzhak, Dmitry Smon, Idan Aizenberg and Hassan Said Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/eladgl/tourismGuide/issues"
  },
  "homepage": "https://github.com/eladgl/tourismGuide#readme",
  "dependencies": {
    "styled-components": "^6.1.11"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "vite": "^5.3.5"
  }
}
